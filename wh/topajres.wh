CLASS TopAjustaReserva


/*
AtualizacaoSinistro
<sinistro>
    <sinistro_numero>1009308009157</sinistro_numero>
    <id_integracao>1</id_integracao> 
    <tp_integracao>2</tp_integracao>
    <ramo>86</ramo>
    <valor_reserva>0,00</valor_reserva>
    <ajuste_reserva>-0,01</ajuste_reserva>
