/*
*
* Glauber - 04/2016
*
* Classe...........: RetornoEbao
*
* Objetivo.........: Classe utilizada para informar o retorno de uma operação.
*
* Atributos........:
*
*    EXPORT CdRetorno
*    EXPORT NmRetorno
*
*
*
* Metodos..........:
*
*    METHOD Init()
*    METHOD Destroy()
*
* Classes Ligadas..:
*
*
*
*/


CLASS RetornoEbao

    // Atributos publico

    EXPORT CdRetorno
    EXPORT NmRetorno

    // Metodos publico

    PROTOTYPE METHOD Init()             CLASS RetornoEbao
    PROTOTYPE METHOD Destroy()          CLASS RetornoEbao



/*
*
* Glauber - 04/2016
*
* Metodo....: Init()
* Objetivo..: Construtor padrão
*
* Parâmetros:
*
*
* Retorno...:
*             Self
*
*/

METHOD Init() CLASS RetornoEbao


    self:CdRetorno            := 0
    self:NmRetorno            := ''



return(self)


/*
*
* Glauber - 04/2016
*
* Metodo....: Destroy()
* Objetivo..: Destruidor padrão
*
* Parâmetros:
*
*
* Retorno...:
*             nil
*
*/

METHOD Destroy() CLASS RetornoEbao

    self:CdRetorno            := nil
    self:NmRetorno            := nil

return(nil)






