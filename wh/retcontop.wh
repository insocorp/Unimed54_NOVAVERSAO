/*
*
* Glauber - 09/2017
*
* Metodo....: Init()
* Objetivo..: Construtor padrão
*
* Parâmetros:
*
*
* Retorno...:
*             Self
*
*/

METHOD Init() CLASS ConsultaTop

    self:Retorno         := RetornoTopNew()
    self:SinistroNumero  := ''
    self:IdIntegracao    := 0
    self:TpIntegracao    := 0
    self:Ramo            := ''
    self:StatusSinistro  := ''
    self:FlSinistro      := ''
    self:PagtoPendentes  := ''
    self:PagtoAgendados  := ''
    self:TpSinistro      := ''
    self:Uf              := ''
    self:CnpjCpf         := ''
    self:NomeSegurado    := ''
    self:DataSinistro    := Ctod('')
    self:DataBaixa       := Ctod('')
    self:NrApolice       := ''
    self:StatusApolice   := ''
    self:NrProposta      := ''
    self:DataContratacao := Ctod('')
    self:DataPagtoPremio := Ctod('')
    self:CdCorretor      := ''
    self:CdEstipulante   := ''
    self:FlCosseguro     := ''
    self:PcCosseguro     := 0
    self:CnpjCongenere   := ''
    self:NrCobertura     := ''
    self:VlLmi           := 0.00
    self:VlSinistrado    := 0.00
    self:DataContratacao := Ctod('')



return(self)


/*
*
* Glauber - 09/2017
*
* Metodo....: Destroy()
* Objetivo..: Destruidor padrão
*
* Parâmetros:
*
*
* Retorno...:
*             nil
*
*/

METHOD Destroy() CLASS ConsultaTop

    self:Retorno         := nil
    self:SinistroNumero  := nil
    self:IdIntegracao    := nil
    self:TpIntegracao    := nil
    self:Ramo            := nil
    self:StatusSinistro  := nil
    self:FlSinistro      := nil
    self:PagtoPendentes  := nil
    self:PagtoAgendados  := nil
    self:TpSinistro      := nil
    self:Uf              := nil
    self:CnpjCpf         := nil
    self:NomeSegurado    := nil
    self:DataSinistro    := nil
    self:DataBaixa       := nil
    self:NrApolice       := nil
    self:StatusApolice   := nil
    self:NrProposta      := nil
    self:DataContratacao := nil
    self:DataPagtoPremio := nil
    self:CdCorretor      := nil
    self:CdEstipulante   := nil
    self:FlCosseguro     := nil
    self:PcCosseguro     := nil
    self:CnpjCongenere   := nil
    self:NrCobertura     := nil
    self:VlLmi           := nil
    self:VlSinistrado    := nil
    self:DataContratacao := nil


return(nil)



/*
*
* Glauber - 09/2017
*
* Metodo....: toString()
* Objetivo..: Retorna uma String com o formato do objeto.
*
* Parâmetros:
*
*
* Retorno...:
*             string
*
*/

METHOD toString() CLASS ConsultaTop


return "ConsultaTop [[Retorno:" + transform(self:Retorno:toString()) +"]" 
