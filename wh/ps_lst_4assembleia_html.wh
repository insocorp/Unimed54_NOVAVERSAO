         lcNR_PASTA                := '' ,;
         lcDOCUMENTOS              := '' ,;
         lcDT_ASSEMBLEIA           := '' ,;
         lcASSEMBLEIA_ORDEMDIA     := '' ,;
         lcSOCIETARIO_ATO          := '' ,;
         lcPODERES                 := '' ,;
         lcNR_PROTOCOLO            := '' ,;
         lcNR_REGISTRO             := '' ,;
         lcNR_ATA                  := '' ,;
         lcASS_DELIBERACAO  := '' ,;
         lcPUT                     := '',;
         lcOBSERVACAO              := '',;
         lcTMP                     := ''  as string

   local laWHERE  := {} ,;
         laFIELDS := {} ,;
         laJOIN   := {} ,;
         laCOUNT  := {} ,;
         laLIMIT  := {} ,;
         laPAGES  := {} ,;
         laTMP    := {} ,;
         laRES    := {} as array

   local ii           := 0 ,;
         xx           := 0 ,;
         lnGOTO_PAGE  := 0 ,;
         lnLIMIT1     := 0 ,;
         lnLIMIT2     := 0 ,;
         lnMAX_PAGES  := 0 ,;
         lnNR_PAGES   := 0 ,;
         lnPAGE_FIN   := 0 ,;
         lnPAGE_INI   := 0 ,;
         lnROWS_PAGE  := 0 as int

   lcTP_PASTA := WGet('TP_PASTA','C')
   lcNR_PASTA := WGet('NR_PASTA','C')
