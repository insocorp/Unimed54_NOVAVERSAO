   // Glauber 05/01/2011
   AbasPerfil(lcTP_PASTA, {'NR_ATA','DT_ASSEMBLEIA','NR_PROTOCOLO','NR_REGISTRO','SOCIETARIO_ATO','ASSEMBLEIA_DELIBERACAO'})
