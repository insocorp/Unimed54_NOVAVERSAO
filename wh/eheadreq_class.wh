/*
*
* Glauber - 04/2016
*
* Classe...........: HeaderRequest
*
* Objetivo.........: Classe que contém o cabeçalho para requisições.
*
* Atributos........:
*
*    EXPORT TpIntegracao
*    EXPORT SinistroNumero
*    EXPORT Ramo
*
*
* Metodos..........:
*
*    METHOD Init()
*    METHOD Destroy()
*
*
* Classes Ligadas..:
*
*
*
*/


CLASS HeaderRequest

    // Atributos publico

    EXPORT TpIntegracao
    EXPORT SinistroNumero
    EXPORT Ramo

    // Metodos publico

    PROTOTYPE METHOD Init()      CLASS HeaderRequest
    PROTOTYPE METHOD Destroy()   CLASS HeaderRequest




/*
*
* Glauber - 04/2016
*
* Metodo....: Init()
* Objetivo..: Construtor padrão
*
* Parâmetros:
*
*
* Retorno...:
*             Self
*
*/

METHOD Init() CLASS HeaderRequest


    self:TpIntegracao   := 0
    self:SinistroNumero := ''
    self:Ramo           := ''


return(self)


/*
*
* Glauber - 04/2016
*
* Metodo....: Destroy()
* Objetivo..: Destruidor padrão
*
* Parâmetros:
*
*
* Retorno...:
*             nil
*
*/

METHOD Destroy() CLASS HeaderRequest

    self:TpIntegracao   := nil
    self:SinistroNumero := nil
    self:Ramo           := nil

return(nil)





