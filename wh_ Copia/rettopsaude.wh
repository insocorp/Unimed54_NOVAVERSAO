/*
*
* Glauber - 09/2017
*
* Classe...........: RetornoTop
*
* Objetivo.........: Classe de retorno das operações de comunicação com Top Saude.
*
* Atributos........:
*
*
*
*
*
* Metodos..........:
*
*    METHOD Init()
*    METHOD Destroy()
*    METHOD Inicializar()
*    METHOD toString()
*
* Classes Ligadas..:
*
*
*
*/


CLASS RetornoTop

    // Atributos publico

    EXPORT CdRetorno
    EXPORT NmRetorno 


    // Metodos publico

    PROTOTYPE METHOD Init()      CLASS RetornoTop
    PROTOTYPE METHOD Destroy()   CLASS RetornoTop
    PROTOTYPE METHOD toString()  CLASS RetornoTop



/*
*
* Glauber - 09/2017
*
* Metodo....: Init()
* Objetivo..: Construtor padrão
*
* Parâmetros:
*
*
* Retorno...:
*             Self
*
*/

METHOD Init() CLASS RetornoTop

    self:CdRetorno := 0
    self:NmRetorno := ''

return(self)


/*
*
* Glauber - 09/2017
*
* Metodo....: Destroy()
* Objetivo..: Destruidor padrão
*
* Parâmetros:
*
*
* Retorno...:
*             nil
*
*/

METHOD Destroy() CLASS RetornoTop

    self:CdRetorno := nil
    self:NmRetorno := nil


return(nil)


/*
*
* Glauber - 09/2017
*
* Metodo....: toString()
* Objetivo..: Retorna uma String com o objeto
*
* Parâmetros:
*
*
* Retorno...:
*             String
*
*/

METHOD toString() CLASS RetornoTop

return "RetornoTop[CdRetorno=" + transform(self:CdRetorno) +;
       ",NmRetorno=" + transform(self:NmRetorno) + "]"



