CLASS RetTopNew

    // Atributos publico


    EXPORT Retorno
    EXPORT SinistroNumero
    EXPORT IdIntegracao
    EXPORT TpIntegracao
    EXPORT Ramo
    EXPORT Mensagem


    // Metodos publico

    PROTOTYPE METHOD Init()      CLASS RetTopNew
    PROTOTYPE METHOD Destroy()   CLASS RetTopNew
    PROTOTYPE METHOD toString()  CLASS RetTopNew






/*
*
* Glauber - 04/2018
*
* Metodo....: Init()
* Objetivo..: Construtor padrão
*
* Parâmetros:
*
*
* Retorno...:
*             Self
*
*/

METHOD Init() CLASS RetTopNew

    self:Retorno         := RetornoTopNew()
    self:SinistroNumero  := ''
    self:IdIntegracao    := 0
    self:TpIntegracao    := 0
    self:Ramo            := ''
    self:Mensagem        := ''



return(self)


/*
*
* Glauber - 04/2018
*
* Metodo....: Destroy()
* Objetivo..: Destruidor padrão
*
* Parâmetros:
*
*
* Retorno...:
*             nil
*
*/

METHOD Destroy() CLASS RetTopNew

    self:Retorno         := nil
    self:SinistroNumero  := nil
    self:IdIntegracao    := nil
    self:TpIntegracao    := nil
    self:Ramo            := nil
    self:Mensagem        := nil


return(nil)



/*
*
* Glauber - 04/2018
*
* Metodo....: toString()
* Objetivo..: Retorna uma String com o formato do objeto.
*
* Parâmetros:
*
*
* Retorno...:
*             string
*
*/

METHOD toString() CLASS RetTopNew


return "RetTopNew [[Retorno:" + transform(self:Retorno:toString()) +"]"
