   next jj

   for jj=1 to len(laINA)
       aadd(laPUT,'INATIVO - '+alltrim(laINA[jj]))
       aadd(laPUTV,alltrim(laINA[jj]))
   next jj

   WPut('QTDE_DIAS'+'.option',laPUT)
   WPut('QTDE_DIAS'+'.optionvalue',laPUTV)

return (nil)


