   //           ser alterados na pasta Filho.
   aadd(laVAL_CAMPOS,'CIP_EMPRESA')
   aadd(laVAL_CAMPOS,'CD_GRP_ECONOMICO')
   aadd(laVAL_CAMPOS,'FL_TT_LITSCONSORTE')
   
   if !empty(laPastaConfig[2])
